
<%= form_with model: @user do |f| %>
<table>
    <tr><td><%= f.label :username %></td><td><%= f.text_field :username %></td></tr>
    <tr><td></td><td></td></tr>
    <tr><td><%= f.label :password %></td><td> <%= f.password_field :password %></td></tr>
    <tr><td></td><td></td></tr>
    <tr><td><%= f.label :email %></td><td><%= f.text_field :email %></td></tr>
    <tr><td></td><td></td></tr>
    <tr><td><%= f.label :phone %></td><td><%= f.text_field :phone %></td></tr>
    <tr><td></td><td></td></tr>
    <tr><td><%= f.label :is_admin %></td><td><%= f.check_box :is_admin %></td></tr>
    <tr><td></td><td></td></tr>
    <tr><td></td><td> <%= f.submit %></td></tr>
</table>
<% end %>